# Testing Environment Configuration
# Used for automated testing and CI/CD

# API Keys (use test/mock keys)
AI_VIDEO_EDITOR_GEMINI_API_KEY=test_key_or_mock
AI_VIDEO_EDITOR_IMAGEN_API_KEY=test_key_or_mock
AI_VIDEO_EDITOR_GOOGLE_CLOUD_PROJECT=test_project

# Testing Settings
AI_VIDEO_EDITOR_DEBUG=false
AI_VIDEO_EDITOR_LOG_LEVEL=WARNING

# Testing-optimized Processing Settings
AI_VIDEO_EDITOR_MAX_CONCURRENT_PROCESSES=1
AI_VIDEO_EDITOR_MAX_MEMORY_USAGE_GB=2.0
AI_VIDEO_EDITOR_ENABLE_GPU_ACCELERATION=false

# Testing Project Settings
AI_VIDEO_EDITOR_DEFAULT_CONTENT_TYPE=general
AI_VIDEO_EDITOR_DEFAULT_QUALITY=low
AI_VIDEO_EDITOR_DEFAULT_OUTPUT_FORMAT=mp4

# Testing Performance Settings (minimal for speed)
AI_VIDEO_EDITOR_WHISPER_MODEL_SIZE=tiny
AI_VIDEO_EDITOR_VIDEO_PROCESSING_TIMEOUT=60
AI_VIDEO_EDITOR_API_REQUEST_TIMEOUT=5
AI_VIDEO_EDITOR_MAX_RETRIES=1

# Testing Paths
AI_VIDEO_EDITOR_OUTPUT_DIR=workspace/temp/testing/outputs
AI_VIDEO_EDITOR_TEMP_DIR=workspace/temp/testing
AI_VIDEO_EDITOR_LOGS_DIR=workspace/temp/testing/logs

# Testing-specific Settings
AI_VIDEO_EDITOR_USE_MOCK_APIS=true
AI_VIDEO_EDITOR_SKIP_EXPENSIVE_OPERATIONS=true
AI_VIDEO_EDITOR_ENABLE_TEST_MODE=true