# Development Environment Configuration
# Copy to .env in project root for development

# API Keys (required for full functionality)
AI_VIDEO_EDITOR_GEMINI_API_KEY=your_gemini_api_key_here
AI_VIDEO_EDITOR_IMAGEN_API_KEY=your_imagen_api_key_here
AI_VIDEO_EDITOR_GOOGLE_CLOUD_PROJECT=your_project_id_here

# Development Settings
AI_VIDEO_EDITOR_DEBUG=true
AI_VIDEO_EDITOR_LOG_LEVEL=DEBUG

# Development-optimized Processing Settings
AI_VIDEO_EDITOR_MAX_CONCURRENT_PROCESSES=1
AI_VIDEO_EDITOR_MAX_MEMORY_USAGE_GB=4.0
AI_VIDEO_EDITOR_ENABLE_GPU_ACCELERATION=false

# Development Project Settings
AI_VIDEO_EDITOR_DEFAULT_CONTENT_TYPE=general
AI_VIDEO_EDITOR_DEFAULT_QUALITY=medium
AI_VIDEO_EDITOR_DEFAULT_OUTPUT_FORMAT=mp4

# Development Performance Settings (faster for testing)
AI_VIDEO_EDITOR_WHISPER_MODEL_SIZE=base
AI_VIDEO_EDITOR_VIDEO_PROCESSING_TIMEOUT=600
AI_VIDEO_EDITOR_API_REQUEST_TIMEOUT=10
AI_VIDEO_EDITOR_MAX_RETRIES=2

# Development Paths
AI_VIDEO_EDITOR_OUTPUT_DIR=workspace/outputs
AI_VIDEO_EDITOR_TEMP_DIR=workspace/temp
AI_VIDEO_EDITOR_LOGS_DIR=workspace/logs